{
  "expo": {
    "name": "tab-app",
    "slug": "tab-app",
    "version": "1.0.0",
    "scheme": "tab-app",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/3d-truck.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/rrg-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/3d-truck.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/rrg-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.arelean.tabapp"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8f71b81d-1d97-4042-b890-dfd6bd988141"
      }
    }
  }
}
